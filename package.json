{
  "name": "ai_mastery_challenge",
  "version": "1.0.0",
  "description": "AI Mastery Challenge project with modern Node.js and ES7+ features",
  "main": "cli/index.js",
  "bin": {
    "mycli": "cli/index.js"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node cli/index.js",
    "dev": "node --watch cli/index.js",
    "dev:server": "cd server && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev:frontend\" --names \"server,frontend\" --prefix-colors \"blue,magenta\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:cli": "vitest tests/cli",
    "test:server": "vitest tests/server",
    "test:server:unit": "vitest tests/server/unit",
    "test:server:integration": "vitest tests/server/integration"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djphillyg/ai_mastery_challenge.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/djphillyg/ai_mastery_challenge/issues"
  },
  "homepage": "https://github.com/djphillyg/ai_mastery_challenge#readme",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "prettier": "^3.6.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "lodash": "^4.17.21",
    "supertest": "^7.1.4",
    "uuid": "^13.0.0"
  }
}
